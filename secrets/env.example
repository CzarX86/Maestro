# Configuração de secrets para o orquestrador
# Copie este arquivo para .env e configure suas chaves

# Gemini CLI
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gpt-4o

# Codex CLI
CODEX_API_KEY=your_codex_api_key_here
CODEX_MODEL=claude-3-5-sonnet

# Cursor CLI
CURSOR_API_KEY=your_cursor_api_key_here

# Configurações de ambiente
NODE_ENV=development
PYTHONPATH=src

# Configurações de logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Configurações de segurança
SANITIZE_LOGS=true
MASK_SECRETS=true

# Configurações de timeout (em segundos)
PLANNER_TIMEOUT=120
CODER_TIMEOUT=300
TESTER_TIMEOUT=600

# Configurações de limites
MAX_DIFF_LINES=1000
MIN_COVERAGE=70
